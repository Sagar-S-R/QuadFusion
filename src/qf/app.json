{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
  "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-local-authentication"],
    "experiments": {
      "typedRoutes": true
    },
    "routes": {
      "tabs": {
        "analytics": "analytics",
        "camera": "camera",
        "index": "index",
        "sensors": "sensors",
        "settings": "settings",
        "scan": "scan",
        "monitor": "monitor"
      }
    }
  }
}
